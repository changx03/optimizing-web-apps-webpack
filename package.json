{
  "name": "solitaire",
  "private": true,
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "core-js": "^3.0.1",
    "numeral": "^2.0.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-loader": "^8.0.5",
    "bower": "^1.8.2",
    "clean-webpack-plugin": "^2.0.1",
    "colors": "^1.3.3",
    "eslint": "^4.9.0",
    "http-server": "^0.11.1",
    "jasmine-core": "^2.8.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-junit-reporter": "^1.2.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "scripts": {
    "postinstall": "node node_modules/bower/bin/bower install",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js --single-run",
    "build": "set NODE_ENV=development&& webpack",
    "build-unix": "NODE_ENV=development webpack",
    "build:prod": "set NODE_ENV=production&& webpack",
    "build:prod-unix": "NODE_ENV=production webpack",
    "watch": "npm run build -- --watch",
    "start": "set NODE_ENV=development&& nodemon -w webpack.config.js -x webpack-dev-server",
    "start-unit": "NODE_ENV=development nodemon -w webpack.config.js -x webpack-dev-server"
  }
